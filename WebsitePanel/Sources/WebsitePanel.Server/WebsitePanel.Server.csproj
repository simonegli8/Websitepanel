  <!-- generate non WSE versions of all services -->
  <Target Name="BeforeBuildNoWSEService" />
  <ItemGroup Condition=" '$(Engine)' == 'MSBuild' ">
    <AsmxServices Include="**\*.asmx" Exclude="$(NoWSEPath)**\*.asmx">
      <InProject>false</InProject>
    </AsmxServices>
    <AsmxServicesCode Include="**\*.asmx.cs" Exclude="$(NoWSEPath)**\*.asmx.cs">
      <InProject>false</InProject>
    </AsmxServicesCode>
    <AsmxServicesNoWSE Include="@(AsmxServices -> '$(NoWSEPath)%(RecursiveDir)%(Filename)%(Extension)')">
      <InProject>false</InProject>
    </AsmxServicesNoWSE>
    <AsmxServicesNoWSECode Include="@(AsmxServicesCode -> '$(NoWSEPath)%(RecursiveDir)%(Filename)%(Extension)')">
      <InProject>false</InProject>
    </AsmxServicesNoWSECode>
  </ItemGroup>
  <Target Condition=" '$(Engine)' == 'MSBuild' " Name="BuildNoWSEServices" Inputs="@(AsmxServices);@(AsmxServicesCode)" Outputs="@(AsmxServicesNoWSE);@(AsmxServicesNoWSECode)">
    <Message Text="Build non WSE services." />
    <MakeDir Directories="$(NoWSEPath)" />
    <!-- copy all services to NoWSE folder -->
    <Copy SourceFiles="@(AsmxServices)" DestinationFiles="@(AsmxServices -> '$(NoWSEPath)%(RecursiveDir)%(Filename)%(Extension)')" />
    <Copy SourceFiles="@(AsmxServicesCode)" DestinationFiles="@(AsmxServicesCode -> '$(NoWSEPath)%(RecursiveDir)%(Filename)%(Extension)')" />
    <!-- change namespace of non WSE services -->
    <FileUpdate Files="@(AsmxServicesNoWSECode)" Regex="([\n]?)([ \t]*)(namespace[ \t\r\n]*)([a-zA-Z0-9.]+)" ReplacementText="$1// This file is auto generated, do not edit.&#xD;&#xA;&#xD;&#xA;$2$3$4.NoWSE" />
    <!-- comment out reference to Microsoft.Web.Service and [Policy(...)] -->
    <FileUpdate Files="@(AsmxServicesNoWSECode)" Regex="((?&lt;=^|[ \t\r\n])using[ \t\r\n]+Microsoft.Web.Services[23]?[^;]*;)|(\[Policy[^\]]*\])" ReplacementText="/* $0 */" />
    <!-- change namespace in .asmx files -->
    <FileUpdate Files="@(AsmxServicesNoWSE)" Regex="([ \t]Class=[&quot;'])(([a-zA-Z0-9]+[.])*)([a-zA-Z0-9]+)([&quot;'])" ReplacementText="$1$2NoWSE.$4$5" />
    <!-- remove outer #if Net condition -->
    <FileUpdate Files="@(AsmxServicesNoWSECode)" Regex="^(?&lt;head&gt;[ \t\n\r]*(((//[^\n\r]*)|(/\*.*\*/))[ \t\r\n]*)*)(#if Net[ \t]*[\r\n]{1,2})(?&lt;body&gt;.*)(#endif[ \t\r\n]*)$" ReplacementText="${head}${body}" Singleline="true" />
    <!-- embrace regular services with #if Net when needed -->
    <FileUpdate Files="@(AsmxServicesCode)" Regex="^(?!.*#if[ \t]+Net[ \n\r\t].*$)(?&lt;head&gt;[ \t\n\r]*((//[^\n\r]*[ \t\r\n]*)|(/\*.*\*/[ \t\r\n]*))*)(?&lt;body&gt;.*Microsoft\.Web\.Services([ \t\n\r]*[^ \t\r\n]+)*)([ \t\n\r]*)$" ReplacementText="${head}#if Net&#xD;&#xA;&#xD;&#xA;${body}&#xD;&#xA;&#xD;&#xA;#endif" Singleline="true" />
  </Target>
